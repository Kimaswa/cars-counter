
let saveEl = document.getElementById("save-el")
let countEl = document.getElementById("count-el")
let count = 0
console.log(saveEl)

function increment() {
    count += 1
    countEl.innerHTML = count
}

function save() {
    let countstr = count + " - " 
    saveEl.innerHTML += countstr
    countEl.innerHTML = 0
    count = 0
   
}

